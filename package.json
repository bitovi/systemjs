{
  "name": "steal-systemjs",
  "version": "0.17.2",
  "description": "System loader extension for flexible AMD & CommonJS support",
  "main": "dist/system.src.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/systemjs/systemjs"
  },
  "author": "Guy Bedford",
  "license": "MIT",
  "dependencies": {
    "steal-es6-module-loader": "^0.17.0"
  },
  "devDependencies": {
    "babel-core": "^4.7.16",
    "qunit": "^0.6.2",
    "uglify-js": "~2.4.13",
    "traceur": "0.0.87"
  },
  "scripts": {
